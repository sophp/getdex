#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules/@module-federation/cli/bin/node_modules:/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules/@module-federation/cli/node_modules:/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules/@module-federation/node_modules:/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules:/Users/sophis.beaupuis/CODE/Learn/node_modules:/Users/sophis.beaupuis/CODE/node_modules:/Users/sophis.beaupuis/node_modules:/Users/node_modules:/node_modules:/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules/@module-federation/cli/bin/node_modules:/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules/@module-federation/cli/node_modules:/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules/@module-federation/node_modules:/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules:/Users/sophis.beaupuis/CODE/Learn/node_modules:/Users/sophis.beaupuis/CODE/node_modules:/Users/sophis.beaupuis/node_modules:/Users/node_modules:/node_modules:/Users/sophis.beaupuis/CODE/Learn/gtdex/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@module-federation/cli/bin/mf.js" "$@"
else
  exec node  "$basedir/../@module-federation/cli/bin/mf.js" "$@"
fi
